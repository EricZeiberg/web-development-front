- if maps.respond_to? :current_page
    .row
        .col-md-12
            = pagination = paginate maps
- maps.in_groups_of(3, false) do |group|
    .row.maps
        - group.each do |map|
            .col-md-4.col-sm-6.col-xs-12
                .map-container
                    = render :partial => "maps/map", :locals => {:map => map}
= pagination
